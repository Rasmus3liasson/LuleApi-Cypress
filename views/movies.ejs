<div>
    <li>
        <h2>Titel: <%= movie.title %> </h2>
        <p>Intro: <%- movie.intro %></p>
        <img width="250px" height="500px" src="<%= movie.image.url%>" alt="">
    </li>
</div>
<div class="reviews">
    <% for(var i=0; i < reviews.length; i++) { %>
        <li>
            <h2>Name: <%= reviews[i].author %> </h2>
            <p>Rating: <%= review[i].rating %> </p>
            <p>Comment: <%= review[i].comment %> </p>
        </li>
    <% } %>
</div>
